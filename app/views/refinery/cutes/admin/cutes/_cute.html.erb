<li class='clearfix record <%= cycle("on", "on-hover") %>' id="<%= dom_id(cute) -%>">
  <span class='title'>
    <%= cute.name %>

  </span>
  <span class='actions'>
    
      <%= action_icon(:preview, refinery.cutes_cute_path(cute), t('.view_live_html')) %>
    
    <%= action_icon(:edit,    refinery.edit_cutes_admin_cute_path(cute), t('.edit') ) %>
    <%= action_icon(:delete,  refinery.cutes_admin_cute_path(cute), t('.delete'),
      { class: "cancel confirm-delete",
        data: {confirm: t('message',  scope: 'refinery.admin.delete', title: cute.name)}}  ) %>
  </span>
</li>
