<ul>
  <% if ::Refinery::Cutes::Admin::CutesController.searchable? %>
    <li>
      <%= render '/refinery/admin/search', :url => refinery.cutes_admin_cutes_path %>
    </li>
  <% end %>
  <li>
    <%= action_label :add, refinery.new_cutes_admin_cute_path, t('.create_new') %>
  </li>
<% if !searching? && ::Refinery::Cutes::Admin::CutesController.sortable? && ::Refinery::Cutes::Cute.many? %>
  <li>
    <%= action_label :reorder,
                     refinery.cutes_admin_cutes_path,
                     t('.reorder', what:  "Cutes"), id: 'reorder_action'%>
    <%= action_label :reorder_done,
                     refinery.cutes_admin_cutes_path,
                      t('.reorder_done', what: "Cutes"), id: 'reorder_action_done', class: 'hidden'%>
  </li>
<% end %>
</ul>
